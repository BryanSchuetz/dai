---
title: Active Projects JSON
---
{% assign projects = site.projects | where_exp: "project", "project.end > 2017" %}[
  [
    [
      "Active DAI Projects",
      "Name",
      "Start",
      "End",
      "Solution",
      "Region"
    ],
    {% for project in projects %}[
      "{{ project.title }}",
      {{ project.start }},
      {{ project.end }},
      "{{ project.solutions[0] }}",
      "{{ project.regions[0] }}"
    ]{% unless forloop.last %},{% endunless %}{% endfor %}
  ]
]
